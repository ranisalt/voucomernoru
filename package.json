{
  "name": "voucomernoru",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "mkdir -p public && postcss -u autoprefixer -c postcss.config.json static/index.css -o public/voucomernoru.css",
    "build:js": "mkdir -p public && cp static/index.js public/voucomernoru.js",
    "clean": "rm -r dist",
    "heroku-postbuild": "npm run update",
    "postinstall": "npm run build",
    "start": "node --harmony $NODE_DEBUG_OPTION server.js",
    "test": "xo",
    "update": "node --harmony update.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ranisalt/voucomernoru.git"
  },
  "author": "Ranieri Althoff <ranisalt@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ranisalt/voucomernoru/issues"
  },
  "homepage": "https://github.com/ranisalt/voucomernoru#readme",
  "dependencies": {
    "any-promise": "^1.3.0",
    "bluebird": "^3.5.0",
    "cheerio": "^0.22.0",
    "cloudinary": "^1.7.1",
    "debug": "^2.6.1",
    "koa": "^2.0.1",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^2.0.1",
    "koa-multer": "^1.0.1",
    "koa-router": "^7.1.0",
    "koa-static": "^3.0.0",
    "nunjucks": "^3.0.0",
    "pdf2table": "0.0.1",
    "redis": "^2.6.5",
    "request": "^2.80.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "postcss-cli": "^3.0.0-beta",
    "xo": "^0.17.1"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "space": true
  }
}
