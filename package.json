{
  "name": "voucomernoru",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "mkdir -p public && postcss static/index.css -u autoprefixer -o public/voucomernoru.css",
    "build:js": "mkdir -p public && cp static/index.js public/voucomernoru.js",
    "clean": "rm -r dist",
    "postinstall": "npm run build",
    "start": "node --harmony $NODE_DEBUG_OPTION server.js",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ranisalt/voucomernoru.git"
  },
  "author": "Ranieri Althoff <ranisalt@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ranisalt/voucomernoru/issues"
  },
  "homepage": "https://github.com/ranisalt/voucomernoru#readme",
  "dependencies": {
    "any-promise": "^1.3.0",
    "cheerio": "^0.22.0",
    "cloudinary": "^1.8.0",
    "debug": "^2.6.4",
    "easymongo": "^5.0.0",
    "koa": "^2.2.0",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^3.0.0",
    "koa-multer": "^1.0.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "nunjucks": "^3.0.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.0",
    "postcss-cli": "^4.0.0",
    "standard": "^10.0.2"
  },
  "engines": {
    "node": ">=7.6.0"
  }
}
