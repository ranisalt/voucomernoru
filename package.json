{
  "name": "voucomernoru",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "mkdir -p public && postcss -u autoprefixer -u postcss-import -c postcss.config.json static/index.css -o public/voucomernoru.css",
    "build:js": "mkdir -p public && cp static/index.js public/voucomernoru.js",
    "clean": "rm -r dist",
    "heroku-postbuild": "npm run update",
    "postinstall": "bower install && npm run build",
    "start": "node --harmony $NODE_DEBUG_OPTION server.js",
    "test": "xo && tap *.spec.js",
    "update": "node --harmony update.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ranisalt/voucomernoru.git"
  },
  "author": "Ranieri Althoff <ranisalt@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ranisalt/voucomernoru/issues"
  },
  "homepage": "https://github.com/ranisalt/voucomernoru#readme",
  "dependencies": {
    "any-promise": "^1.3.0",
    "bluebird": "^3.3.5",
    "cheerio": "^0.20.0",
    "cloudinary": "^1.3.1",
    "debug": "^2.2.0",
    "koa": "^2.0.0",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-multer": "^1.0.0",
    "koa-router": "^7.1.0",
    "koa-static": "^3.0.0",
    "nunjucks": "^2.5.2",
    "pdf2table": "0.0.1",
    "redis": "^2.6.0-2",
    "request": "^2.72.0",
    "request-promise": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "bower": "^1.7.9",
    "postcss-cli": "^2.5.2",
    "postcss-import": "^8.1.2",
    "xo": "^0.15.0"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "no-unused-expressions": 1
    },
    "semicolon": false,
    "space": true
  }
}
