{
  "name": "voucomernoru",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "babel {i,r,s}*.js -d dist && cp -r views dist",
    "heroku-postinstall": "npm run update",
    "start": "npm run build && node dist/server.js",
    "test": "xo && babel-tap *.spec.js",
    "update": "babel update.js -d dist && node dist/update.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ranisalt/voucomernoru.git"
  },
  "author": "Ranieri Althoff <ranisalt@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ranisalt/voucomernoru/issues"
  },
  "homepage": "https://github.com/ranisalt/voucomernoru#readme",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.5",
    "cheerio": "^0.20.0",
    "debug": "^2.2.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.1.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-views": "^5.0.1",
    "mustache": "^2.2.1",
    "redis": "^2.6.0-2",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-tap": "^5.0.0",
    "xo": "^0.15.0"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "no-unused-expressions": 1
    },
    "semicolon": false,
    "space": true
  }
}
